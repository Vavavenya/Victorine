{% extends "base.html.twig" %}
{% block stylesheets %}<link href="css/HomePage.css" rel="stylesheet">{% endblock %}
{% block menu %}
{% endblock %}
{% block body %}
    <div class="container">
     <table class="table table-bordered">
      <thead class="thead-dark">
      <tr>
       <th scope="col">ID</th>
       <th scope="col">UserName</th>
       <th scope="col">Email</th>
      </tr>
      </thead>
       <tbody>
        {% for user in users %}
         <tr>
          <td>{{ user.id}}</td>
          <td>{{ user.username}}</td>
          <td>{{ user.email}}</td>
         </tr>
        {% endfor %}
       </tbody>
     </table>
    </div>
{% endblock %}